; Author class declaration created with Protege (ontology/ontology.pont)
(defclass Author
	(is-a USER)
	(role concrete)
	(single-slot gender
		(type SYMBOL)
		(allowed-values male female)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot popularity
		(type SYMBOL)
		(allowed-values low medium high)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot rating
		(type FLOAT)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot name_
		(type STRING)
	;+		(cardinality 1 1)
		(create-accessor read-write))
)

; Book class declaration created with Protege (ontology/ontology.pont)
(defclass Book
	(is-a USER)
	(role concrete)
	(single-slot popularity
		(type SYMBOL)
		(allowed-values low medium high)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot title
		(type STRING)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot rating
		(type FLOAT)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot year
		(type INTEGER)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot author
		(type INSTANCE)
	;+		(allowed-classes Author)
		(create-accessor read-write))
	(single-slot genre
		(type STRING)
	;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot pages
		(type INTEGER)
	;+		(cardinality 1 1)
		(create-accessor read-write))
)

; Book class declaration whit the score and its reasons to choose it
(defclass BookR
	(is-a USER)
	(role concrete)
	(single-slot book
		(type INSTANCE)
	)
	(single-slot refused-genre
		(type SYMBOL)
		(default FALSE)
	)
	(single-slot score
		(type INTEGER)
	)
	(multislot reasons
		(type STRING)
  )
)

