; Main module
(defmodule MAIN
	(export ?ALL)
)

; User data module
(defmodule DATA
	(import MAIN ?ALL)
	(export ?ALL)
)

; User information module
(defmodule USER
	(import MAIN ?ALL)
	(import DATA ?ALL)
	(export ?ALL)
)

; User preferences module
(defmodule PREF
	(import MAIN ?ALL)
	(import DATA ?ALL)
	(export ?ALL)
)

; Book popularity preference module
(defmodule POPU
	(import MAIN ?ALL)
	(import DATA ?ALL)
	(import PREF ?ALL)
	(export ?ALL)
)

; Book antiquity preference module
(defmodule ANTI
	(import MAIN ?ALL)
	(import DATA ?ALL)
	(import PREF ?ALL)
	(export ?ALL)
)

; Book genre preference module
(defmodule GENR
	(import MAIN ?ALL)
	(import DATA ?ALL)
	(import PREF ?ALL)
	(export ?ALL)
)

; User book recommendation module
(defmodule RECO
	(import MAIN ?ALL)
	(export ?ALL)
)

; Results presentation module
(defmodule PRES
	(import MAIN ?ALL)
	(export ?ALL)
)

